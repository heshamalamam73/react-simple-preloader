{
  "name": "react-simple-preloader",
  "version": "1.1.0",
  "description": "simple react preloader with typeScrypt",
  "main": "lib/index",
  "types": "src/index",
  "typings": "src/index",
  "author": "Mahmoud okily",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf lib/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css lib/",
    "build": "npm run clean && tsc && npm run copy-files",
    "publish": "npm publish"
  },
  "dependencies": {
    "@types/react": "18.0.6",
    "parcel-bundler": "^1.6.1",
    "react": "18.0.0",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "grunt": "~0.4.5",
    "grunt-bump": "^0.8.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "~0.5.0",
    "rimraf": "^3.0.2",
    "typescript": "4.4.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "preloader",
    "spinner",
    "loading",
    "react"
  ]
}
